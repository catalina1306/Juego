script/source = "extends CharacterBody2D

var speed = 300
var gravity = ProjectSettings.get_setting(\"physics/2d/default_gravity\")
var jump_speed = -750
var brazos_count = 4

@onready var _animated_sprite = $AnimatedSprite2D
@export var brazo_morado = preload(\"res://Scenes/brazo_morado.tscn\")
@onready var hombro = $hombro
@onready var hud = $CanvasLayer/hud

var health = 100:
	set(value):
		health = value
		if hud:
			hud.set_health(health)
		
var maxHealth = 100

func _ready() -> void:
	hud.set_health(health)
	
func _process(delta: float) -> void:
	if health <= 0:
		game_over()
	
	var move_input = Input.get_action_strength(\"move_right2\") - Input.get_action_strength(\"move_left2\")

	if move_input > 0:
		if brazos_count == 4:
			_animated_sprite.play(\"run\")
		if brazos_count == 3:
			_animated_sprite.play(\"run-verde\")
		if brazos_count == 2:
			_animated_sprite.play(\"run-morado\")
		if brazos_count == 1:
			_animated_sprite.play(\"run-morado\")
		if brazos_count == 0:
			_animated_sprite.play(\"run-morado\")
		_animated_sprite.flip_h = true
	elif move_input < 0:
		if brazos_count == 4:
			_animated_sprite.play(\"run\")
		if brazos_count == 3:
			_animated_sprite.play(\"run-verde\")
		if brazos_count == 2:
			_animated_sprite.play(\"run-morado\")
		if brazos_count == 1:
			_animated_sprite.play(\"run-morado\")
		if brazos_count == 0:
			_animated_sprite.play(\"run-morado\")
		_animated_sprite.flip_h = false
	else:
		if is_on_floor():
			if brazos_count == 4:
				_animated_sprite.play(\"idle\")
			if brazos_count == 3:
				_animated_sprite.play(\"idle-verde\")
			if brazos_count == 2:
				_animated_sprite.play(\"idle-morado\")
			if brazos_count == 1:
				_animated_sprite.play(\"idle-morado\")
			if brazos_count == 0:
				_animated_sprite.play(\"idle-morado\")
		else:
			_animated_sprite.stop()
		#_animated_sprite.flip_h = false (poner si se quiere que siempre mire a la derecha)
		
		
func _physics_process(delta: float) -> void:
	if !is_on_floor():
		velocity.y += gravity * delta

	var move_input = Input.get_action_strength(\"move_right2\") - Input.get_action_strength(\"move_left2\")

	if Input.is_action_just_pressed(\"lanzar2\") && brazos_count >0:
		lanzar()
		_animated_sprite.play(\"lanzar\")
		brazos_count -=1

	# Handle Jump.
	if Input.is_action_just_pressed(\"move_up2\"):
		if is_on_floor():
			if brazos_count == 4:
				_animated_sprite.play(\"jump\")
			if brazos_count == 3:
				_animated_sprite.play(\"jump-verde\")
			if brazos_count == 2:
				_animated_sprite.play(\"jump-morado\")
			if brazos_count == 1:
				_animated_sprite.play(\"jump-morado\")
			if brazos_count == 0:
				_animated_sprite.play(\"jump-morado\")
			velocity.y = jump_speed

	if move_input != 0:
		velocity.x = move_input * speed
	else:
		velocity.x = 0

	move_and_slide()
	
# Agregar esta función para controlar el último frame de la animación de salto
func _on_AnimatedSprite2D_animation_finished():
	if _animated_sprite.animation == \"jump\":
		_animated_sprite.frame = _animated_sprite.frame_count - 1
	elif _animated_sprite.animation == \"jump-verde\":
		_animated_sprite.frame = _animated_sprite.frame_count - 1
	elif _animated_sprite.animation == \"jump-morado\":
		_animated_sprite.frame = _animated_sprite.frame_count - 1
	elif _animated_sprite.animation == \"lanzar\":
		_animated_sprite.frame = _animated_sprite.frame_count - 1

func lanzar():
	if not brazo_morado:
		return
	if brazos_count > 0:
		var extremidad = brazo_morado.instantiate() as RigidBody2D
		extremidad.add_collision_exception_with(self)
		add_child(extremidad)
		extremidad.global_position = hombro.global_position
		extremidad.apply_central_impulse((Vector2.RIGHT if _animated_sprite.flip_h else Vector2.LEFT)*750)

func game_over():
	mostrar_game_over_scene()
func mostrar_game_over_scene():
	get_tree().change_scene_to_file(\"res://Scenes/GameOverMenu.tscn\")
	
func take_damage():
	if health > 0 :
		health = max(health-10,0)

#func mostrar_menu_emergente():
	# Captura y difumina el fondo
	# Pausa el juego
	#get_tree().paused = true
	# Muestra el menú emergente
	#var popup_menu = preload(\"res://Scenes/GameOver2.tscn\").instantiate()
	#if popup_menu:
	#	get_tree().get_root().add_child(popup_menu)
	#	popup_menu.popup_centered()

	
